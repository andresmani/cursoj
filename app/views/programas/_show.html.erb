<%- model_class = Programa -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human %></h1>
</div>

<div class='programa'>
  <p><b>Nombre: </b><%= @programa.nombre %></p>
  <p><b>Precio: </b><%= @programa.precio %></p>
  <p><b>Observaciones: </b><%= @programa.observaciones %></p>
</div>